
void setup(){
   size(400, 400);
}
void draw(){
 //head
  noStroke();
  fill(#A85848);
  ellipse(200, 150, 100, 100);

//body
  noStroke();
  fill(#A85848);
  ellipse(200, 250, 130, 150);
  
//feet
  noStroke();
  fill(#A85848);
  ellipse(150, 300, 50, 50);
  
  noStroke();
  fill(#A85848);
  ellipse(250, 300, 50, 50);
  
//ears
  fill(#A85848);
  ellipse(160, 125, 50, 50);
  
  fill(#A85848);
  ellipse(240, 125, 50, 50);
 
//eyes
  fill(#371C13);
  ellipse(170, 150, 10, 10);
}

